    "@types/node": "^12.0.7",
    "@types/uuid": "^3.4.4",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "ts-jest": "^24.0.2",
    "ts-mockito": "^2.3.1",
    "ts-node-dev": "^1.0.0-pre.39",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "circleci-api": "^4.0.0",
    "express": "^4.17.1",
